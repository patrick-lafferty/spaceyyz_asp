@model IEnumerable<AdministrationViewModel.UserViewModel>

<h1>Users</h1>

<article>
	<table>
		<tr>
			<th>Username</th>
			<th>First name</th>
			<th>Last name</ht>
			<th>Roles</th>
		</tr>

		@foreach(var user in Model)
		{
			<tr>
				<td>user.Username</td>
				<td>user.Firstname</td>
				<td>user.Lastname</td>
				<td>user.Roles</td>
			</tr>
		}
	</table>
</article>
